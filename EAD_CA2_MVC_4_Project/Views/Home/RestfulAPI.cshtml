@model EAD_CA2_MiniProject.Model.StudentModel
@using System.Web.Helpers;

<link href="https://code.jquery.com/ui/1.12.0-rc.2/themes/smoothness/jquery-ui.css" rel="stylesheet" />

<h1>List of All Students From REST API:</h1>
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                Student Id
            </th>
            <th>
                Student Name
            </th>
            <th>
                Date of Birth
            </th>
            <th>
                Height
            </th>
            <th>
                Weight
            </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in @Model.Students)
        {
            <tr>
                <th scope="row">@item.StudentID</th>
                <td>@item.StudentName</td>
                <td>@item.DateOfBirth</td>
                <td>@item.Height</td>
                <td>@item.Weight</td>
            </tr>
        }
    </tbody>
</table>


<div class="clearfix"></div>

